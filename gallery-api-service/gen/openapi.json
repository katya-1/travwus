{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "Event service",
    "version" : "0.0.1"
  },
  "servers" : [ {
    "url" : "http://localhost:10010/api/v1"
  }, {
    "url" : "https://localhost:10010/api/v1"
  } ],
  "paths" : {
    "/events" : {
      "get" : {
        "description" : "Returns list of exhibition of paintings on weekly",
        "operationId" : "events",
        "parameters" : [ {
          "name" : "date",
          "in" : "query",
          "description" : "Filtering date",
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Success",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/EventsResponse"
                }
              }
            }
          },
          "default" : {
            "description" : "Error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "x-swagger-router-controller" : "event-controller"
    },
    "/swagger" : {
      "x-swagger-pipe" : "swagger_raw"
    }
  },
  "components" : {
    "schemas" : {
      "EventsResponse" : {
        "type" : "array",
        "items" : {
          "$ref" : "#/components/schemas/Event"
        }
      },
      "Event" : {
        "required" : [ "id" ],
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "string"
          },
          "name" : {
            "type" : "string"
          },
          "theme" : {
            "type" : "string"
          },
          "price" : {
            "type" : "integer"
          },
          "location" : {
            "type" : "string"
          },
          "randomDate" : {
            "type" : "string"
          }
        }
      },
      "ErrorResponse" : {
        "required" : [ "message" ],
        "type" : "object",
        "properties" : {
          "message" : {
            "type" : "string"
          }
        }
      }
    }
  }
}