{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "Hello our Gallery service",
    "version" : "0.1.0"
  },
  "servers" : [ {
    "url" : "http://localhost:10010/api/v1"
  } ],
  "paths" : {
    "/today" : {
      "get" : {
        "description" : "Returns today's exhibition",
        "operationId" : "todays",
        "parameters" : [ {
          "name" : "date",
          "in" : "query",
          "description" : "Filtering date",
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Success",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/TodaysResponse"
                }
              }
            }
          },
          "default" : {
            "description" : "Error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "x-swagger-router-controller" : "gallery-controller"
    },
    "/swagger" : {
      "x-swagger-pipe" : "swagger_raw"
    }
  },
  "components" : {
    "schemas" : {
      "TodaysResponse" : {
        "type" : "array",
        "items" : {
          "$ref" : "#/components/schemas/Today"
        }
      },
      "Today" : {
        "required" : [ "id" ],
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "string"
          },
          "title" : {
            "type" : "string"
          },
          "artist" : {
            "type" : "string"
          },
          "start-time" : {
            "type" : "string"
          },
          "price" : {
            "type" : "string"
          },
          "location" : {
            "type" : "string"
          },
          "discription" : {
            "type" : "string"
          }
        }
      },
      "ErrorResponse" : {
        "required" : [ "message" ],
        "type" : "object",
        "properties" : {
          "message" : {
            "type" : "string"
          }
        }
      }
    }
  }
}